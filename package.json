{
  "name": "modernmern-frontend",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build-only": "next build",
    "start": "next start",
    "build-stats": "cross-env ANALYZE=true npm run build",
    "export": "next export",
    "build-prod": "run-s clean build-only",
    "clean": "rimraf .next out",
    "lint": "next lint",
    "check-types": "tsc --noEmit --pretty",
    "test": "jest",
    "check-backend": "curl --silent http://127.0.0.1:4000/health || echo 'Please start ModernMERN backend or something goes wrong in the backend'",
    "cypress": "cypress open --e2e",
    "cypress:headless": "cypress run",
    "e2e": "start-server-and-test check-backend http://127.0.0.1:4000/health dev http://127.0.0.1:3000 cypress",
    "e2e:headless": "start-server-and-test check-backend http://127.0.0.1:4000/health dev http://127.0.0.1:3000 cypress:headless",
    "prepare": "husky install"
  },
